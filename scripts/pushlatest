#!/bin/bash
docker rmi toussii/domoticz:latest-x86-64
docker rmi toussii/domoticz:latest-arm32v7
docker rmi toussii/domoticz:latest-arm64v8

docker pull toussii/domoticz:latest-x86-64
docker pull toussii/domoticz:latest-arm32v7
docker pull toussii/domoticz:latest-arm64v8

docker manifest create \
toussii/domoticz:latest \
--amend toussii/domoticz:latest-x86-64 \
--amend toussii/domoticz:latest-arm32v7 \
--amend toussii/domoticz:latest-arm64v8

docker manifest push toussii/domoticz:latest
